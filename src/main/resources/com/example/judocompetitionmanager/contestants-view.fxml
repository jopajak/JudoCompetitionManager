<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="scrollPane" minHeight="0.0" minWidth="0.0" prefHeight="398.0" prefWidth="604.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.judocompetitionmanager.ContestantsController">
   <children>
      <TextField layoutX="167.0" layoutY="45.0">
         <effect>
            <InnerShadow />
         </effect></TextField>
      <Label layoutX="63.0" layoutY="49.0" text="Find someone" />
      <Label fx:id="bigLabel" layoutX="241.0" layoutY="14.0" text="List of my contestants" />
      <Button layoutX="373.0" layoutY="45.0" mnemonicParsing="false" onAction="#onAddClick" prefHeight="25.0" prefWidth="161.0" text="Add a new contestant">
         <effect>
            <InnerShadow />
         </effect></Button>
      <ScrollPane layoutX="84.0" layoutY="118.0" prefHeight="202.0" prefWidth="451.0">
         <content>
            <ListView fx:id="contestantsList" prefHeight="200.0" prefWidth="449.0" />
         </content>
      </ScrollPane>
      <ImageView fx:id="goBack" fitHeight="40.0" fitWidth="43.0" layoutX="14.0" layoutY="344.0" onMouseClicked="#goBack" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@go-back.png" />
