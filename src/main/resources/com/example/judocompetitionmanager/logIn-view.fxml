<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="login" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.judocompetitionmanager.LogInController">
   <children>
      <Label id="iconLabel" alignment="CENTER" layoutX="1.0" prefHeight="43.0" prefWidth="600.0" text="JUDO COMPETITION MANAGER" />
      <Label contentDisplay="CENTER" layoutX="135.0" layoutY="145.0" text="Login" />
      <Label contentDisplay="CENTER" layoutX="125.0" layoutY="179.0" text="Password" />
      <PasswordField fx:id="passwordField" alignment="CENTER" layoutX="229.0" layoutY="175.0">
         <effect>
            <InnerShadow />
         </effect>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor></PasswordField>
      <TextField fx:id="nameTextField" alignment="CENTER" layoutX="229.0" layoutY="141.0">
         <effect>
            <InnerShadow />
         </effect></TextField>
      <Button alignment="CENTER" layoutX="229.0" layoutY="325.0" mnemonicParsing="false" onAction="#switchToMainScene" prefHeight="25.0" prefWidth="70.0" text="Log In">
         <effect>
            <InnerShadow />
         </effect></Button>
      <Button contentDisplay="CENTER" layoutX="308.0" layoutY="325.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="70.0" text="Sign In">
         <effect>
            <InnerShadow />
         </effect></Button>
      <Label fx:id="info" layoutX="3.0" layoutY="80.0" prefHeight="17.0" prefWidth="594.0" />
   </children>
</AnchorPane>
